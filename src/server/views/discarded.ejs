<!DOCTYPE html>
<html lang="en">
<%- include('header.ejs') %>
<body>
  <%- include('head.ejs', {isJobRunning}) %>
  <table>
    <thead>
      <th>Title</th>
      <th>Description</th>
      <th>Delete</th>
    </thead>
    <tbody>
      <% discarded.forEach(function(item) { %>
        <tr>
          <td><%= item.title %></td>
          <td><%= item.description %></td>
          <td><button onclick="deleteDiscarded(item.id)">X</button></td>
        </tr>
      <% }); %>
    </thead>
  </table>
</body>
</html>